<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.o_mysql.mapper.OHeapTypeMapper">

    <resultMap id="BaseResultMap" type="org.example.o_mysql.domain.OHeapType">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="length" column="length" jdbcType="BIGINT"/>
            <result property="width" column="width" jdbcType="BIGINT"/>
            <result property="dec" column="dec" jdbcType="VARCHAR"/>
            <result property="createtime" column="createTime" jdbcType="TIMESTAMP"/>
            <result property="updatetime" column="updateTime" jdbcType="TIMESTAMP"/>
            <result property="isDelete" column="is_delete" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,length,
        width,dec,createTime,
        updateTime,is_delete
    </sql>
</mapper>
